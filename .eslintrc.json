{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "import/prefer-default-export": "off"
    // "quotes": [
    //     "error",
    //     "double"
    // ],
  }
}
